         <attribute name="ident">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">supplies
               a name which functions as the prefix for an abbreviated pointing scheme such as a
               private URI scheme. The prefix constitutes the text preceding the first
               colon.</a:documentation>
            <data type="token">
               <param name="pattern">[a-z][a-z0-9\+\.\-]*</param>
            </data>
         </attribute>
         <empty/>
      </element>
   </define>
