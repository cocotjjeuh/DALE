               <sch:report xmlns="http://www.tei-c.org/ns/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  xmlns:s="http://www.ascc.net/xml/schematron"
                  test="@target and count( child::* ) &gt; 0">If the
                     <rng:attribute>@target</rng:attribute> on <sch:name/> is used, the
                     <rng:element>relatedItem</rng:element> element must be empty</sch:report>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tei_enrich-relatedItem-targetorcontent1-constraint-assert-9">
