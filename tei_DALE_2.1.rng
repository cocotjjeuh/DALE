            manuscript or other object. [10.7.1. Object Description]</a:documentation>
         <group>
            <ref name="support"/>
            <element name="extent">
               <group>
                  <text/>
                  <oneOrMore>
                     <group>
                        <optional>
                           <element name="locus">
                              <choice>
                                 <data type="token">
                                    <param name="pattern">f.\s\d{1,3}(v|r|va|vb|ra|rb)?</param>
                                 </data>
                                 <data type="token">
                                    <param name="pattern"
                                       >ff.\s\d{1,3}(v|r|va|vb|ra|rb)?-\d{1,3}(v|r|va|vb|ra|rb)?</param>
                                 </data>
                                 <data type="token">
                                    <param name="pattern">s.f.</param>
                                 </data>
                                 <data type="token">
                                    <param name="pattern">f.\s\p{Nl}(v|r|va|vb|ra|rb)?</param>
                                 </data>
                                 <data type="token">
                                    <param name="pattern"
                                       >ff.\s\p{Nl}(v|r|va|vb|ra|rb)?-\p{Nl}(v|r|va|vb|ra|rb)?</param>
                                 </data>
                              </choice>
                              <attribute name="from">
                                 <choice>
                                    <data type="token">
                                       <param name="pattern">\d{3}(v|r|va|vb|ra|rb)?</param>
                                    </data>
                                    <data type="token">
                                       <param name="pattern">0</param>
                                    </data>
                                    <data type="token">
                                       <param name="pattern">\p{Nl}(v|r|va|vb|ra|rb)?</param>
                                    </data>
                                 </choice>
                              </attribute>
                              <attribute name="to">
                                 <choice>
                                    <data type="token">
                                       <param name="pattern">\d{3}(v|r|va|vb|ra|rb)?</param>
                                    </data>
                                    <data type="token">
                                       <param name="pattern">0</param>
                                    </data>
                                    <data type="token">
                                       <param name="pattern">\p{Nl}(v|r|va|vb|ra|rb)?</param>
                                    </data>
                                 </choice>
                              </attribute>
                           </element>
                        </optional>
                        <element name="dimensions">
