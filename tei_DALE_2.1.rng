            <ref name="supportDesc"/>
            <ref name="layoutDesc"/>
         </group>
         <ref name="att.global.attributes"/>
         <attribute name="form">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">a short
               project-specific name identifying the physical form of the carrier, for example as a
               codex, roll, fragment, partial leaf, cutting etc.</a:documentation>
            <choice>
               <value>codex</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">a
                  bound codex</a:documentation>
               <value>fragment</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">a
                  physical fragment</a:documentation>
            </choice>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="supportDesc">
      <element name="supportDesc">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(support
