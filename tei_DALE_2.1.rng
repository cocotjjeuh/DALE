   <define name="history">
      <element name="history">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">groups
            elements describing the full history of a manuscript, manuscript part, or other object.
            [10.8. History]</a:documentation>
         <zeroOrMore>
            <ref name="model.pLike"/>
         </zeroOrMore>
         <group>
            <ref name="origin"/>
            <ref name="provenance"/>
            <ref name="acquisition"/>
         </group>
         <ref name="att.global.attributes"/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tei_enrich-history-locus-constraint-assert">
            <rule context="tei:history//tei:locus[@from='0']">
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  xmlns:s="http://www.ascc.net/xml/schematron" test="@to='0' and .='s.f.'">The
                  content of <rng:element>locus</rng:element> doesn't match the value of its
                  attributes.</sch:assert>
            </rule>
            <rule context="tei:history//tei:locus[@from=@to and starts-with(@from,'00')]">
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  xmlns:s="http://www.ascc.net/xml/schematron"
                  test=" substring-after(.,'f. ')=substring-after(@from,'00')">The content of
                     <rng:element>locus</rng:element> doesn't match the value of its
                  attributes.</sch:assert>
            </rule>
            <rule context="tei:history//tei:locus[@from=@to and starts-with(@from,'0')]">
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  xmlns:s="http://www.ascc.net/xml/schematron"
                  test=" substring-after(.,'f. ')=substring-after(@from,'0')">The content of
                     <rng:element>locus</rng:element> doesn't match the value of its
                  attributes.</sch:assert>
            </rule>
            <rule context="tei:history//tei:locus[@from=@to]">
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  xmlns:s="http://www.ascc.net/xml/schematron"
                  test=" substring-after(.,'f. ')=@from">The content of
                     <rng:element>locus</rng:element> doesn't match the value of its
                  attributes.</sch:assert>
            </rule>
            <rule
               context="tei:history//tei:locus[starts-with(@from,'00') and starts-with(@to,'00')]">
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  xmlns:s="http://www.ascc.net/xml/schematron"
                  test="(substring-after(.,'-')=substring-after(@to,'00')) and (substring-before(substring-after(.,'ff. '),'-')=substring-after(@from,'00'))"
                  >The content of <rng:element>locus</rng:element> doesn't match the value of its
                  attributes.</sch:assert>
            </rule>
            <rule context="tei:history//tei:locus[starts-with(@from,'00') and starts-with(@to,'0')]">
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  xmlns:s="http://www.ascc.net/xml/schematron"
                  test="(substring-after(.,'-')=substring-after(@to,'0')) and (substring-before(substring-after(.,'ff. '),'-')=substring-after(@from,'00'))"
                  >The content of <rng:element>locus</rng:element> doesn't match the value of its
                  attributes.</sch:assert>
            </rule>
            <rule context="tei:history//tei:locus[starts-with(@from,'00')]">
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  xmlns:s="http://www.ascc.net/xml/schematron"
                  test="(substring-after(.,'-')=@to) and (substring-before(substring-after(.,'ff. '),'-')=substring-after(@from,'00'))"
                  >The content of <rng:element>locus</rng:element> doesn't match the value of its
                  attributes.</sch:assert>
            </rule>
            <rule context="tei:history//tei:locus[starts-with(@from,'0') and starts-with(@to,'0')]">
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  xmlns:s="http://www.ascc.net/xml/schematron"
                  test="(substring-after(.,'-')=substring-after(@to,'0')) and (substring-before(substring-after(.,'ff. '),'-')=substring-after(@from,'0'))"
                  >The content of <rng:element>locus</rng:element> doesn't match the value of its
                  attributes.</sch:assert>
            </rule>
            <rule context="tei:history//tei:locus[starts-with(@from,'0')]">
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  xmlns:s="http://www.ascc.net/xml/schematron"
                  test="(substring-after(.,'-')=@to) and (substring-before(substring-after(.,'ff. '),'-')=substring-after(@from,'0'))"
                  >The content of <rng:element>locus</rng:element> doesn't match the value of its
                  attributes.</sch:assert>
            </rule>
            <rule context="tei:history//tei:locus[not(@from=@to)]">
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  xmlns:s="http://www.ascc.net/xml/schematron"
                  test="substring-after(.,'-')=@to and contains(.,@from)">The content of
                     <rng:element>locus</rng:element> doesn't match the value of its
                  attributes.</sch:assert>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tei_enrich-history-locus-value-constraint-assert">
            <rule context="tei:history//tei:locus">
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  xmlns:s="http://www.ascc.net/xml/schematron" test="@to>=@from">The value of
                     <rng:attribute>@to</rng:attribute> should be greater than or equal to the value
                  of <rng:attribute>@from</rng:attribute>.</sch:assert>

            </rule>
         </pattern>
      </element>
   </define>
   <define name="origin">
      <element name="origin">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains any
            descriptive or other information concerning the origin of a manuscript, manuscript part,
            or other object. [10.8. History]</a:documentation>
         <oneOrMore>
            <element name="p">
               <group>
                  <element name="origDate">
                     <data type="token">
                        <param name="pattern"
                           >saec.\s(I|V|X)+(\s(in.|ex.))?(\-(I|V|X)+)?(\s(in.|ex.))?</param>
                     </data>
                     <ref name="att.datable.attributes"/>
                     <ref name="att.editLike.attributes"/>
                     <ref name="att.typed.attributes"/>
                     <optional>
                        <ref name="att.dimensions.attributes"/>
                     </optional>
                  </element>
                  <ref name="origPlace"/>
                  <interleave>
                     <zeroOrMore>
                        <element name="locus">
                           <choice>
                              <data type="token">
                                 <param name="pattern">f.\s\d{1,3}(v|r|va|vb|ra|rb)?</param>
                              </data>
                              <data type="token">
                                 <param name="pattern"
                                    >ff.\s\d{1,3}(v|r|va|vb|ra|rb)?-\d{1,3}(v|r|va|vb|ra|rb)?</param>
                              </data>
                              <data type="token">
                                 <param name="pattern">s.f.</param>
                              </data>
                              <data type="token">
                                 <param name="pattern">f.\s\p{Nl}(v|r|va|vb|ra|rb)?</param>
                              </data>
                              <data type="token">
                                 <param name="pattern"
                                    >ff.\s\p{Nl}(v|r|va|vb|ra|rb)?-\p{Nl}(v|r|va|vb|ra|rb)?</param>
                              </data>
                           </choice>
                           <attribute name="from">
                              <choice>
                                 <data type="token">
                                    <param name="pattern">\d{3}(v|r|va|vb|ra|rb)?</param>
                                 </data>
                                 <data type="token">
                                    <param name="pattern">0</param>
                                 </data>
                                 <data type="token">
                                    <param name="pattern">\p{Nl}(v|r|va|vb|ra|rb)?</param>
                                 </data>
                              </choice>
                           </attribute>
                           <attribute name="to">
                              <choice>
                                 <data type="token">
                                    <param name="pattern">\d{3}(v|r|va|vb|ra|rb)?</param>
                                 </data>
                                 <data type="token">
                                    <param name="pattern">0</param>
                                 </data>
                                 <data type="token">
                                    <param name="pattern">\p{Nl}(v|r|va|vb|ra|rb)?</param>
                                 </data>
                              </choice>
                           </attribute>
                        </element>
                     </zeroOrMore>
                     <zeroOrMore>
                        <ref name="date"/>
                     </zeroOrMore>
                     <zeroOrMore>
                        <ref name="quote"/>
                     </zeroOrMore>
                     <zeroOrMore>
                        <ref name="persName"/>
                     </zeroOrMore>
                     <zeroOrMore>
                        <ref name="orgName"/>
                     </zeroOrMore>
                     <zeroOrMore>
                        <ref name="placeName"/>
                     </zeroOrMore>
                     <text/>
                  </interleave>
                  <ref name="att.editLike.attributes"/>
                  <ref name="att.datable.attributes"/>
               </group>
               <ref name="att.global.attributes"/>
               <ref name="att.declaring.attributes"/>
               <ref name="att.fragmentable.attributes"/>
               <ref name="att.written.attributes"/>
            </element>
         </oneOrMore>

         <empty/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tei_enrich-origin-attributes-assert">
            <rule context="tei:origin/tei:p/tei:origDate">
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  xmlns:s="http://www.ascc.net/xml/schematron"
                  test="(@notBefore and @notAfter) or @when">The
                     <rng:attribute>@notBefore</rng:attribute> and
                     <rng:attribute>@notAfter</rng:attribute> (or
                     <rng:attribute>@when</rng:attribute>) of <rng:element>origDate</rng:element>
                  are required.</sch:assert>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tei_enrich-origin-attributes-restrictions-assert">
            <rule context="tei:origin/tei:p/tei:origDate">
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  xmlns:s="http://www.ascc.net/xml/schematron" test="@notAfter>@notBefore">The value
                  of <rng:attribute>@notAfter</rng:attribute> should be greater than the value of
                     <rng:attribute>@notBefore</rng:attribute> of
                     <rng:element>origDate</rng:element>.</sch:assert>
            </rule>
         </pattern>

      </element>
   </define>
