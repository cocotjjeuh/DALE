   <define name="att.declaring.attribute.decls">
      <optional>
         <attribute name="decls">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
               >identifies one or more declarable elements within the header, which are understood
               to apply to the element bearing this attribute and its content.</a:documentation>
