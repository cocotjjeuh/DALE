                        </msItem>
                        <msItem n="3">
                            <locus from="061r" to="074v">ff. 61r-74v</locus>
                            <title type="sub">Physiognomonia</title>
                            <author ref="persin:aristoteles">Aristoteles</author>
                            <respStmt>
                                <resp>Translator</resp>
                                <persName ref="persin:bartholomaeusdemessana">Bartholomaeus de
                                    Messana</persName>
                            </respStmt>
                            <incipit/>
                            <quote/>
