                    <msContents>
                        <msItem n="1">
                            <locus from="001r" to="034r">ff. 1r-34r</locus>
                            <title type="sub">De Anima</title>
                            <author ref="persin:aristoteles">Aristoteles</author>
                            <respStmt>
                                <resp>Commentator</resp>
                                <persName ref="persin:themistius">Themistius</persName>
                            </respStmt>
                            <incipit/>
                            <quote/>
                            <explicit/>
                            <colophon/>
                            <note type=""/>
                        </msItem>
                        <msItem n="2">
                            <locus from="035r" to="062r">ff. 35r-62r</locus>
                            <title type="sub">De Anima</title>
                            <author ref="persin:aristoteles">Aristoteles</author>
                            <respStmt>
                                <resp>Commentator</resp>
                                <persName ref="">Jacobus de Duaco</persName>
                            </respStmt>
                            <incipit>Sicut dicitur in principio Phisicorum, illa que sunt communia et magis confusa sunt bonis primo nota</incipit>
                            <quote/>
                            <explicit>illud ad quod nati sunt.</explicit>
                            <colophon/>
                            <note type="desc">Questiones Jacobi de Duaco in De Anima.</note>
                        </msItem>
                        <msItem n="3">
                            <locus from="062v" to="082v">ff. 62v-82v</locus>
                            <title type="sub">Meteorologica</title>
                            <author ref="persin:aristoteles">Aristoteles</author>
                            <respStmt>
                                <resp>Commentator</resp>
                                <persName ref="persin:anonymus">Anonymus</persName>
                            </respStmt>
                            <incipit>Sicut dicit Apuleus et Philosophus in libro De Mundo, corpus humanum</incipit>
                            <quote/>
                            <explicit>(ex abrupto) erit spiritus in quo non erit mare.</explicit>
                            <colophon/>
                            <note type="desc">Questiones in Metheora.</note>
                        </msItem>
                        <msItem n="4">
                            <locus from="083r" to="129r">ff. 83r-129r</locus>
                            <title type="sub">Physica</title>
                            <author ref="persin:aristoteles">Aristoteles</author>
                            <respStmt>
                                <resp>Commentator</resp>
                                <persName ref="persin:anonymus">Anonymus</persName>
                            </respStmt>
                            <incipit>Quoniam quidem intelligere etc. Sicut dicit Eustratius super primum moralium</incipit>
                            <quote/>
                            <explicit>(ex abrupto ?) movebitur ex se omnia mota.</explicit>
                            <colophon/>
                            <note type="desc">f. 129 bis r.</note>
                            <note type="desc">Questiones in Physicam.</note>
                        </msItem>
                        <msItem n="5">
                            <locus from="130r" to="164v">ff. 130r-164v</locus>
                            <title type="sub">Ethica Nicomachea</title>
                            <author ref="persin:aristoteles">Aristoteles</author>
                            <respStmt>
                                <resp>Commentator</resp>
                                <persName ref="persin:anonymus">Anonymus</persName>
                            </respStmt>
                            <incipit>Quamvis scriptum sit ab Alexandro quod viri philosophici</incipit>
                            <quote/>
                            <explicit>(explicit ex abrupto) sed interiores passiones quare, etc.</explicit>
                            <colophon/>
                            <note type="desc">Lemmata ex libro Ethicorum.</note>
                            <note type="desc">Questiones in Ethicam.</note>
                        </msItem>
                    </msContents>
