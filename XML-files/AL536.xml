                     <incipit/>
                     <quote/>
                     <explicit>Explicit liber secundus... De Causis Proprietatum Elementorum... et
                        hic terminatur opus istud <date when="1286-09">anno ab Incarnatione Domini
                           M.CC°. octogesimo sexto, mense septembris</date></explicit>
                     <colophon/>
                     <note type=""/>
                  </msItem>
