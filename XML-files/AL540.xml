                    <msContents>
                        <msItem n="1">
                            <locus from="001r" to="091r">ff. 1r-91r</locus>
                            <title type="sub">Quodlibeta</title>
                            <author ref="persin:petrusdealvernia">Petrus de Alvernia</author>
                            <respStmt>
                                <resp/>
                                <persName ref=""></persName>
                            </respStmt>
                            <incipit/>
                            <quote/>
                            <explicit/>
                            <colophon/>
                            <note type="person">In calce: <quote>Disputate a magistro <persName ref="persin:petrusdealvernia">Petro de Alvernia</persName>, canonico Parisiensi a. D. <date when="1301">M°CCC° primo</date></quote>.</note>
                        </msItem>
                        <msItem n="2">
                            <locus from="092r" to="127v">ff. 92r-127v</locus>
                            <title type="sub">Quaestiones</title>
                            <author ref="persin:aegidiusaugustinensis">Aegidius Augustinensis</author>
                            <respStmt>
                                <resp/>
                                <persName ref=""></persName>
                            </respStmt>
                            <incipit/>
                            <quote/>
                            <explicit/>
                            <colophon/>
                            <note type="person"><quote>Questiones disputate a fratre <persName ref="persin:aegidiusaugustinensis">Egidio Augustinense</persName></quote>.</note>
                        </msItem>
                        <msItem n="3">
                            <locus from="127v" to="163v">ff. 127v-163v</locus>
                            <title type="sub">Meteorologica</title>
                            <author ref="persin:aristoteles">Aristoteles</author>
                            <respStmt>
                                <resp>Commentator</resp>
                                <persName ref="persin:albertusmagnus">Albertus Magnus</persName>
                            </respStmt>
                            <incipit/>
                            <quote/>
                            <explicit/>
                            <colophon/>
                            <note type="desc">Vacant <locus from="164" to="167">ff. 164-167</locus>.</note>
                        </msItem>
                        <msItem n="4">
                            <locus from="168r" to="172r">ff. 168r-172r</locus>
                            <title type="sub">De Quatuor Virtutibus</title>
                            <author ref="persin:martinusbracarensis">Martinus Bracarensis</author>
                            <respStmt>
                                <resp/>
                                <persName ref=""></persName>
                            </respStmt>
                            <incipit>Quatuor virtutum species</incipit>
                            <quote/>
                            <explicit>pugniat (sic) ignaviam.</explicit>
                            <colophon/>
                            <note type="person">Ps.-Seneca.</note>
                        </msItem>
                        <msItem n="5">
                            <locus from="172r" to="184v">ff. 172r-184v</locus>
                            <title type="sub">Epistolae Senecae</title>
                            <author ref="persin:anonymus">Anonymus</author>
                            <respStmt>
                                <resp/>
                                <persName ref=""></persName>
                            </respStmt>
                            <incipit>Mentis colonus et cultor</incipit>
                            <quote/>
                            <explicit/>
                            <colophon/>
                            <note type="desc">Finis deest.</note>
                        </msItem>
                        <msItem n="6">
                            <locus from="185r" to="229v">ff. 185r-229v</locus>
                            <title type="sub">Problemata</title>
                            <author ref="persin:aristoteles">Aristoteles</author>
                            <respStmt>
                                <resp>Translator</resp>
                                <persName ref="persin:bartholomaeusdemessana">Bartholomaeus de Messana</persName>
                            </respStmt>
                            <incipit/>
                            <quote/>
                            <explicit/>
                            <colophon/>
                            <note type="person">Tr. vulgata.</note>
                            <note type="desc">In fine excerpta tantum.</note>
                        </msItem>
                    </msContents>
